{
  "name": "angular12-storybook6",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "preview-dist": "http-server dist/angular12-storybook6",
    "storybook": "start-storybook -c src/.storybook -p 6006",
    "build-storybook": "build-storybook -c src/.storybook"
  },
  "architect": {
    "storybook": {
      "builder": "@storybook/angular:start-storybook",
      "options": {
        "port": 6006,
        "browserTarget": "angular12-storybook6:build"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@storybook/addon-docs": "^7.4.6",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "rfs": "^10.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular/cli": "~12.2.17",
    "@angular/compiler-cli": "~12.2.0",
    "@storybook/addon-actions": "^6.3.0-beta.11",
    "@storybook/addon-essentials": "^6.3.0-beta.11",
    "@storybook/addon-links": "^6.3.0-beta.11",
    "@storybook/angular": "^6.3.0-beta.11",
    "@storybook/builder-webpack5": "^6.3.0-beta.11",
    "@storybook/manager-webpack5": "^6.3.0-beta.11",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.2.3"
  }
}
